<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>ShoopingCart</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>RegisterUser</servlet-name>
    <servlet-class>com.shoopingcart.controllers.RegisterUser</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Login</servlet-name>
    <servlet-class>com.shoopingcart.controllers.Login</servlet-class>
    </servlet>
    <servlet>
  	<servlet-name>LoginDao</servlet-name>
    <servlet-class>com.shoopingcart.controllers.LoginDao</servlet-class>
    </servlet>
    <servlet>
  	<servlet-name>Logout</servlet-name>
    <servlet-class>com.shoopingcart.controllers.Logout</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegisterUser</servlet-name>
    <url-pattern>/registeruser</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/loginn</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>LoginDao</servlet-name>
    <url-pattern>/logindao</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>Logout</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
</web-app>